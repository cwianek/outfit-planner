{
  "name": "outfits",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/outfits/src",
  "prefix": "outfit-planner-mf",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/outfits",
        "index": "apps/outfits/src/index.html",
        "main": "apps/outfits/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/outfits/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/outfits/src/favicon.ico",
          "apps/outfits/src/assets",
          {
            "input": "libs/shared/assets",
            "glob": "**/*",
            "output": "assets"
          }
        ],
        "styles": [
          "apps/outfits/src/styles.scss",
          "node_modules/bootstrap/dist/css/bootstrap.min.css",
          "node_modules/primeicons/primeicons.css",
          "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
          "node_modules/primeng/resources/primeng.min.css"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/outfits/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/outfits/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-dev-server",
      "configurations": {
        "production": {
          "browserTarget": "outfits:build:production"
        },
        "development": {
          "browserTarget": "outfits:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4202,
        "publicHost": "http://outfits-dev"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "outfits:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/outfits/**/*.ts",
          "apps/outfits/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/outfits/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nrwl/angular:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "outfits:build",
        "port": 4202
      },
      "configurations": {
        "development": {
          "buildTarget": "outfits:build:development"
        },
        "production": {
          "buildTarget": "outfits:build:production"
        }
      }
    }
  },
  "tags": []
}
